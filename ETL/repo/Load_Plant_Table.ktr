        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;Script here
var inputflowertime = flowering_time;
var inputseed       = seed_ripens;
var inputleaf       = in_leaf;

&#47;&#47;Set the Regular Expressions
var strSingleReg = &quot;^([0-9]+)$&quot;;
var strMultipleReg = &quot;^([0-9]+) - ([0-9]+)$&quot;;

&#47;&#47;Apply more regular expressions
&#47;&#47; Flowering Time
var ResultSingleflower   =  str2RegExp(inputflowertime, strSingleReg);
var ResultMultipleflower =  str2RegExp(inputflowertime, strMultipleReg);
&#47;&#47; Seed Ripening Time
var ResultSingleseed     =  str2RegExp(inputseed, strSingleReg);
var ResultMultipleseed   =  str2RegExp(inputseed, strMultipleReg);
&#47;&#47;Leaf Times
var ResultSingleleaf     =  str2RegExp(inputleaf, strSingleReg);
var ResultMultipleleaf   =  str2RegExp(inputleaf, strMultipleReg);

&#47;&#47;set the default values
